    "allowSyntheticDefaultImports": true,
    "allowJs": true,
    "esModuleInterop": true,
    "target": "ES2020",
    "outDir": "dist-tsc"
  },
  "include": ["vite.config.js"],
  "exclude": ["dist", "node_modules"]
}
